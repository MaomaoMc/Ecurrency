{
    "extends": "airbnb",
    "plugins": [
        "import",
        "react"
    ],
    "env": {
      "browser": 1
    },
    "rules": {
      "prefer-template": 0,
      "no-console": 0
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    }
}
